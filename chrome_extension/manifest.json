{
  "manifest_version": 2,
  "name": "Binge!",
  "description": "Automatically click 'Continue Watching' in Netflix",
  "version": "1.0.2",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts":[
    {
      "matches": ["*://*.netflix.com/*"],
      "js": ["arrive-2.0.0.min.js", "clicker.js"],
      "run_at" : "document_end"
    }
  ]
}